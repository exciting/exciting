                 <xs:documentation>writes the elsectric field to file.</xs:documentation>
               </xs:annotation>
               <xs:complexType>
                 <xs:all>
                   <xs:element ex:status="expert" ex:unit="" ref="plot2d" minOccurs="0" />
                   <xs:element ex:status="expert" ex:unit="" ref="plot3d" minOccurs="0" />
                 </xs:all>
